{
  "name": "vue-black-dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "A sample admin dashboard based on black dashboard UI template",
  "author": "",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "postinstall": "if test \"$NODE_ENV\" = \"production\" ; then npm run build ; fi ",
    "start": "node server.js",
    "lint": "vue-cli-service lint",
    "dev": "npm run serve",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run dev"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "chart.js": "^2.9.4",
    "express": "^4.17.1",
    "register-service-worker": "^1.5.2",
    "toastr": "^2.1.4",
    "trae": "^1.4.2",
    "vee-validate": "^3.1.0",
    "vue": "^2.6.6",
    "vue-axios": "^2.1.5",
    "vue-chartjs": "^3.4.0",
    "vue-form-wizard": "^0.8.4",
    "vue-i18n": "^8.8.1",
    "vue-router": "^3.0.2",
    "vue-router-prefetch": "^1.4.0",
    "vue-upload-component": "^2.8.20",
    "vue-upload-multiple-image": "^1.1.4",
    "vue2-transitions": "^0.2.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-pwa": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-i18n": "^0.5.1",
    "vue-template-compiler": "^2.6.6"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
